  "scripts": {
    "jenkins": "bower -f install && npm prune && npm i && npm run build",
    "jenkins:commit": "npm run jenkins && npm run test:report",
    "jenkins:merge": "npm run jenkins && npm run docs && npm prune --production",
    "build": "npm run clean && npm run transpile && gulp scss:production",
    "clean": "gulp clean",
    "docs:api": "npm run transpile && jsdoc -r node_modules/app -d build/docs",
    "docs": "gitbook build",
    "lint": "eslint app",
    "debug": "npm run build && node-debug bin/www",
    "dev": "gulp",
    "server": "node bin/www",
    "server:selenium": "selenium-standalone install --silent && selenium-standalone start",
    "transpile": "gulp babel webpack",
    "pretest": "npm run transpile",
    "test": "gulp test",
    "test:report": "NODE_PATH=app mocha --compilers js:babel-core/register --recursive test/unit --reporter xunit > build/reports.xml",
    "test:browser": "wdio wdio.conf.js",
    "test:browser:docker": "test/browser/bin/run",
    "test:component": "mocha --require test/component/lib/domSetup.js --compilers js:babel-core/register --recursive test/component/specs",
