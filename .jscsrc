{
 "esnext": true,
 "preset": "airbnb",
 "excludeFiles": [
   "app/routes.js",
   "app/components/**",
   "app/main/components",
   "app/modules/**/components",
   "node_modules/**",
   "public/javascript/**",
   "public/vendor/**"
  ],
  "requirePaddingNewLinesAfterBlocks": {
    "allExcept": ["inCallExpressions"]
  }
}
